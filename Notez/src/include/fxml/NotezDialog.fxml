<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0"
	styleClass="background" stylesheets="@Stylesheets.css"
	prefWidth="319.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<ToolBar fx:id="toolBar" nodeOrientation="RIGHT_TO_LEFT"
			styleClass="background-darker" stylesheets="@Stylesheets.css"
			BorderPane.alignment="CENTER">
			<items>
				<Button fx:id="btnClose" mnemonicParsing="false" onAction="#close"
					styleClass="background-darker" stylesheets="@Stylesheets.css" text="" />
				<Label fx:id="lblTitle" styleClass="background-darker" stylesheets="@Stylesheets.css" text="" />
			</items>
		</ToolBar>
	</top>
	<center>
		<Label text="Label" fx:id="lblMsg" BorderPane.alignment="CENTER" styleClass="background"
			stylesheets="@Stylesheets.css" />
	</center>
	<bottom>
		<HBox fx:id="hBoxButtons" alignment="TOP_RIGHT" BorderPane.alignment="CENTER" styleClass="background"
							stylesheets="@Stylesheets.css">
			<children>
				<Button fx:id="btnYes" mnemonicParsing="false" text="Yes" onAction="#yes" styleClass="background" stylesheets="@Stylesheets.css"/>
				<Button fx:id="btnNo" mnemonicParsing="false" text="No" onAction="#no" styleClass="background" stylesheets="@Stylesheets.css"/>
				<Button fx:id="btnCancel" mnemonicParsing="false" text="Cancel" onAction="#cancel" styleClass="background" stylesheets="@Stylesheets.css"/>
			</children>
		</HBox>
	</bottom>
</BorderPane>
